    Map accounts = [
            "dev"     : "179438333696",
            "qa"      : "179438333696"
    ]

    Map clusters = [
            "dev"     : "ecsclusterdev",
            "qa"      : "ecsclusterprod"
    ]



            pipeline{
                agent any
                environment {
                    AWS_DEFAULT_REGION = 'us-east-1'
                    AWS_ACCOUNT_ID = accounts.get('dev')
                    ECS_CLUSTER = clusters.get('dev')
                    REPOSITORY_URI = "${AWS_ACCOUNT_ID}.dkr.ecr.${AWS_DEFAULT_REGION}.amazonaws.com"
                    DOCKER_IMAGE_TAG_PROGRAM = "${REPOSITORY_URI}/program-service:latest"

                    AWS_ECR_REPO = "program-service"
                    ECS_SERVICE = "Programservice"
                    SNS_TOPIC_ARN = "arn:aws:sns:${AWS_DEFAULT_REGION}:${AWS_ACCOUNT_ID}:jenkins"

                }
                stages{
                        stage('Initialization'){
                            steps{
                                sh """
                                echo "AWS_ACCOUNT_ID = ${AWS_ACCOUNT_ID}"
                                echo "ECS_CLUSTER    = ${ECS_CLUSTER}"
                                """
                        }}
                    }
            }